<p id="notice"><%= notice %></p>

<h1>Your Albums</h1>

    <div>

    <% @albums.each do |album| %>
      
        
        <%= album.name %>
        <%= link_to 'Show', album %>
        <%= link_to 'Edit', edit_album_path(album) %>
        <%= link_to 'Delete', album, method: :delete, data: { confirm: 'Are you sure?' } %>

    </div>
      
    <% end %>

<br>

<%= link_to 'Create an Album', new_album_path %>





